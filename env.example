# Environment variables for PikaHelper RAG System

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=pikadb
POSTGRES_USER=pika_user
POSTGRES_PASSWORD=pika_pass

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=password123

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Service URLs
EMBEDDING_SERVICE_URL=http://localhost:8001
CHATBOT_API_URL=http://localhost:8000
CHATBOT_UI_URL=http://localhost:3000

# Airflow Configuration
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://pika_user:pika_pass@postgres:5432/pikadb
AIRFLOW__CORE__DAGS_ARE_PAUSED_AT_CREATION=true
AIRFLOW__CORE__LOAD_EXAMPLES=false
